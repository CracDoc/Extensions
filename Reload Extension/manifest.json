{
    "manifest_version": 3,
    "name": "Auto Reloader",
    "version": "1.3",
    "description": "Reload pages automatically with a countdown on the extension icon.",
    "permissions": ["storage", "tabs", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  